{
    "bookname": "json-config-name",     
    "outputfile": "SUMMARY.md",     
    "catalog": "all", 
    "ignores": [],     
    "unchanged": [], 
    
    "title": "Sample GitBook",
    "author": "Yao Zhu <me@yaozhu.top>",
    "description": "This is a sample book created by gitbook",
    "extension": null,
    "generator": "site",
    "isbn": "null",
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        },
        "sidebar": {
            "Yaozhu's Blog": "http://www.yaozhu.top"
        }
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "anchor-navigation-ex", 
        "tbfed-pagefooter",
        "copy-code-button",
        "donate",
        "hide-element",
        "expandable-chapters-small",
        "github-buttons",
        "editlink",
        "copy-code-button",
        "-lunr", "-search", "search-pro",
        "splitter"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": false,  
            "showGoTop":true,
            "associatedWithSummary": true,
            "printLog": false,  
            "multipleH1": true, 
            "mode": "float",
            "float": {
                "floatIcon": "fa fa-navicon", 
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },

        "donate": {
            "wechat": "/assets/wechat.png",
            "alipay": "/assets/alipay.jpg",
            "title": "喝咖啡吧",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "tbfed-pagefooter": {
            "copyright":"&Copyright &copy <a href='https://yaozhu.top' target='_blank'>朋友请坐吧</a> 2023",
            "modify_label": "本文档更新于：",
            "modify_format": "YYYY-MM-DD"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "github-buttons": {       
            "buttons": [
                { "user": "juoyo",         
                  "repo": "oexambook-in-writing",         
                  "type": "star",         
                  "size": "large" 
                }, 
                { "user": "juoyo",        
                  "type": "follow",         
                  "width": "150",        
                  "count": true
                }]     
        },
        "editlink": {       
            "base": "https://github.com/juoyo/oexambook-in-writing",       
            "label": "Edit This Page",       
            "multilingual": false     
        }
    },
    "variables": {}
}